<% var mine = LOOKUP.ownsCharacter(session.user, character.id) %>

<!-- Page Heading -->
<div class="row heading page-header">
	<div class="col-lg-12">
		<h1>
			<span data-mirrored="name"><%= (!character.secrets.name || session.user.dm || mine) ? (character.name || "") : "Unknown" %></span>
			<small>Character</small>
		</h1>
	</div>
</div>
<!-- /.row -->

<div class="row">
	<div class="col-xs-12">

		<div class="row character wide <% if(mine) { %>editable<% } else { %>display<% } %>">
			<div class="charsheet col-md-12">
				<div class="row profile">
					<div class="col-md-12">
						<h2 class="text-center">Profile</h2>
						<hr />
					</div>
					<!------------------------------------>
					<!--		Avatar					-->
					<!------------------------------------>
					<div class="col-md-3 text-center">
						<% if(mine) { %><a href="#"><% } %>
							<img id="image" class="avatar" width="200" src="<%= character.avatar || "" %>" alt="" data-id="<%= character.avatar.split("images/")[1] || "" %>">
							<div class="imageError"></div>
							<% if(mine) { %></a><% } %>
						<form id="uploadAvatar">
							<input id="imageUpload" class="hidden" type="file" accept="image/*" />
						</form>
					</div>
					<div class="col-md-9">
						<div class="row">
							<div class="vspace-19"></div>
							<!------------------------------------>
							<!--		Name					-->
							<!------------------------------------>
							<div class="col-md-6">
								<% if(mine || (character.name && (!character.secrets.name || session.user.dm))) { %>
								<div><small>NAME<% if(mine || session.user.dm) { %> <span id="secret_name" class="secrecy"><i class="fa fa-circle<% if(!character.secrets.name) { %>-o<% } %>"></i> Secret</span><% } %></small></div>
								<div id="name" data-mirrored="name"<% if(mine) { %> contenteditable<% } %>><%= character.name || "" %></div>
								<% } %>
							</div>
							<!------------------------------------>
							<!--		Alignment				-->
							<!------------------------------------>
							<div class="col-md-4">
								<% if(mine || (character.alignment && (!character.secrets.alignment || session.user.dm))) { %>
								<div><small>ALIGNMENT<% if(mine || session.user.dm) { %> <span id="secret_alignment" class="secrecy"><i class="fa fa-circle<% if(!character.secrets.alignment) { %>-o<% } %>"></i> Secret</span><% } %></small></div>
								<div id="alignment"<% if(mine) { %> contenteditable<% } %>><%= character.alignment || "" %></div>
								<% } %>
							</div>
							<!------------------------------------>
							<!--		Gender					-->
							<!------------------------------------>
							<div class="col-md-2">
								<% if(mine || (character.gender && (!character.secrets.gender || session.user.dm))) { %>
								<div><small>GENDER<% if(mine || session.user.dm) { %> <span id="secret_gender" class="secrecy"><i class="fa fa-circle<% if(!character.secrets.gender) { %>-o<% } %>"></i> Secret</span><% } %></small></div>
								<div id="gender"<% if(mine) { %> contenteditable<% } %>><%= character.gender || "" %></div>
								<% } %>
							</div>
						</div>
						<div class="row">
							<div class="vspace-19"></div>
							<!------------------------------------>
							<!--		Character Level			-->
							<!------------------------------------>
							<div class="col-md-4">
								<% if(mine || (character.level && (!character.secrets.level || session.user.dm))) { %>
								<div><small>CHARACTER CLASS/LEVEL<% if(mine || session.user.dm) { %> <span id="secret_level" class="secrecy"><i class="fa fa-circle<% if(!character.secrets.level) { %>-o<% } %>"></i> Secret</span><% } %></small></div>
								<div id="level"<% if(mine) { %> contenteditable<% } %>><%= character.level || "" %></div>
								<% } %>
							</div>
							<!------------------------------------>
							<!--		Deity					-->
							<!------------------------------------>
							<div class="col-md-4">
								<% if(mine || (character.deity && (!character.secrets.deity || session.user.dm))) { %>
