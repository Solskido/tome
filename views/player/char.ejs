									<div id="am_str" class="attr calculated"></div>
									<div id="am_dex" class="attr calculated"></div>
									<div id="am_con" class="attr calculated"></div>
									<div id="am_int" class="attr calculated"></div>
									<div id="am_wis" class="attr calculated"></div>
									<div id="am_cha" class="attr calculated"></div>
