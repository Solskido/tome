								<div id="race"<% if(mine) { %> contenteditable<% } %>><%= character.race || "" %></div>
								<% } %>
							</div>
							<!------------------------------------>
							<!--		Size					-->
							<!------------------------------------>
							<div class="col-md-2">
								<% if(mine || (character.size && (!character.secrets.size || session.user.dm))) { %>
								<div><small>SIZE<% if(mine || session.user.dm) { %> <span id="secret_size" class="secrecy"><i class="fa fa-circle<% if(!character.secrets.size) { %>-o<% } %>"></i> Secret</span><% } %></small></div>
								<div id="size"<% if(mine) { %> contenteditable<% } %>><%= character.size || "" %></div>
								<% } %>
							</div>
							<!------------------------------------>
							<!--		Age						-->
							<!------------------------------------>
							<div class="col-md-2">
								<% if(mine || (character.age && (!character.secrets.age || session.user.dm))) { %>
								<div><small>AGE<% if(mine || session.user.dm) { %> <span id="secret_age" class="secrecy"><i class="fa fa-circle<% if(!character.secrets.age) { %>-o<% } %>"></i> Secret</span><% } %></small></div>
								<div id="age"<% if(mine) { %> contenteditable<% } %>><%= character.age || "" %></div>
								<% } %>
							</div>
							<!------------------------------------>
							<!--		Height					-->
							<!------------------------------------>
							<div class="col-md-2">
								<% if(mine || (character.height && (!character.secrets.height || session.user.dm))) { %>
								<div><small>HEIGHT<% if(mine || session.user.dm) { %> <span id="secret_height" class="secrecy"><i class="fa fa-circle<% if(!character.secrets.height) { %>-o<% } %>"></i> Secret</span><% } %></small></div>
								<div id="height"<% if(mine) { %> contenteditable<% } %>><%= character.height || "" %></div>
								<% } %>
							</div>
							<!------------------------------------>
							<!--		Weight					-->
							<!------------------------------------>
							<div class="col-md-2">
								<% if(mine || (character.weight && (!character.secrets.weight || session.user.dm))) { %>
								<div><small>WEIGHT<% if(mine || session.user.dm) { %> <span id="secret_weight" class="secrecy"><i class="fa fa-circle<% if(!character.secrets.weight) { %>-o<% } %>"></i> Secret</span><% } %></small></div>
								<div id="weight"<% if(mine) { %> contenteditable<% } %>><%= character.weight || "" %></div>
								<% } %>
							</div>
							<!------------------------------------>
							<!--		Eyes					-->
							<!------------------------------------>
							<div class="col-md-2">
								<% if(mine || (character.eyes && (!character.secrets.eyes || session.user.dm))) { %>
								<div><small>EYES<% if(mine || session.user.dm) { %> <span id="secret_eyes" class="secrecy"><i class="fa fa-circle<% if(!character.secrets.eyes) { %>-o<% } %>"></i> Secret</span><% } %></small></div>
								<div id="eyes"<% if(mine) { %> contenteditable<% } %>><%= character.eyes || "" %></div>
								<% } %>
							</div>
						</div>
					</div>
					<!------------------------------------>
					<!--		Bio						-->
					<!------------------------------------>
					<div class="col-md-12">
						<% if(mine || (character.bio && (!character.secrets.bio || session.user.dm ))) { %>
						<div><small>BIOGRAPHY<% if(mine || session.user.dm) { %> <span id="secret_bio" class="secrecy"><i class="fa fa-circle<% if(!character.secrets.bio) { %>-o<% } %>"></i> Secret</span><% } %></small></div>
						<div id="bio" class="expandable"<% if(mine) { %> contenteditable<% } %>><%= character.bio || "" %></div>
						<% } %>
					</div>
					<!------------------------------------>
					<!--		Notes					-->
					<!------------------------------------>
					<div class="col-md-12">
						<% if(mine) { %>
						<div><small>NOTES (VISIBLE ONLY BY YOU)</small></div>
						<div id="notes" class="expandable" contenteditable><%= character.notes || "" %></div>
						<% } %>
