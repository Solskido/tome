								<div id="homeland"<% if(mine) { %> contenteditable<% } %>><%= character.homeland || "" %></div>
								<% } %>
							</div>
						</div>
						<div class="row">
							<div class="vspace-19"></div>
							<!------------------------------------>
							<!--		Race					-->
							<!------------------------------------>
							<div class="col-md-2">
