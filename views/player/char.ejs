								<div id="deity"<% if(mine) { %> contenteditable<% } %>><%= character.deity || "" %></div>
								<% } %>
							</div>
							<!------------------------------------>
							<!--		Homeland				-->
							<!------------------------------------>
							<div class="col-md-4">
								<% if(mine || (character.homeland && (!character.secrets.homeland || session.user.dm))) { %>
