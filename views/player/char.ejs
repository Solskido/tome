									<div id="ta_con" class="attr tempas" contenteditable><%= character.charsheet.ta_con || "" %></div>
									<div id="ta_int" class="attr tempas" contenteditable><%= character.charsheet.ta_int || "" %></div>
									<div id="ta_wis" class="attr tempas" contenteditable><%= character.charsheet.ta_wis || "" %></div>
									<div id="ta_cha" class="attr tempas" contenteditable><%= character.charsheet.ta_cha || "" %></div>
								</div>
								<div class="col-md-3 attrcol">
									<div class="attr top"><small>TEMP MODIFIER</small></div>
									<div id="tm_str" class="attr calculated"></div>
									<div id="tm_dex" class="attr calculated"></div>
									<div id="tm_con" class="attr calculated"></div>
									<div id="tm_int" class="attr calculated"></div>
									<div id="tm_wis" class="attr calculated"></div>
									<div id="tm_cha" class="attr calculated"></div>
								</div>
							</div>
						</div>
						<div class="col-md-5">
							<!------------------------------------>
							<!--		Hit Points				-->
							<!------------------------------------>
							<div class="row attrrow">
								<div class="col-md-4 attrcol">
									<div class="hp left legend">HP<br /><small>HIT POINTS</small></div>
								</div>
								<div class="col-md-8 attrcol">
									<div class="row attrrow">
										<div class="col-md-7 attrcol">
											<div id="maxhp" contenteditable><%= character.charsheet.maxhp || "" %></div>
											<span class="inputbuddy">TOTAL</span>
										</div>
										<div class="col-md-5 attrcol">
											<div id="dr" contenteditable><%= character.charsheet.dr || "" %></div>
											<span class="inputbuddy">DR</span>
										</div>
									</div>
								</div>
							</div>
							<div class="row attrrow">
								<div class="col-md-12 attrcol">
									<div><small>WOUNDS / CURRENT HP</small></div>
									<div id="currhp" contenteditable><%= character.charsheet.currhp || "" %></div>
								</div>
							</div>
							<div class="row attrrow">
								<div class="col-md-12 attrcol">
									<div><small>NONLETHAL DAMAGE</small></div>
									<div id="nlhp" contenteditable><%= character.charsheet.nlhp || "" %></div>
